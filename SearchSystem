		ArrayList<Recipe> recipeList = new ArrayList<Recipe>();
		return recipeList;
	}
	
	public ArrayList<Recipe> applianceSearch(){
	ArrayList<Recipe> recipeList = new ArrayList<Recipe>();
	return recipeList;
	}
	public ArrayList<Recipe> timeSearch(){
		ArrayList<Recipe> recipeListTime = new ArrayList<Recipe>();
		return recipeListTime;

	}
	

}
